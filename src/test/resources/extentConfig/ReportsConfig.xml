<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->

		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Automation</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Automation Report for Deck Designer</reportName>

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportHeadline>Test Result</reportHeadline>

		<!-- global date format override -->
		<!-- defaults to yyyy-MM-dd -->
		<dateFormat>yyyy-MM-dd</dateFormat>

		<!-- global time format override -->
		<!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<testViewChartLocation>TOP</testViewChartLocation>

		<!-- <testViewChartLocation>bottom</testViewChartLocation> -->

		<!-- custom javascript -->
		<scripts>
            <![CDATA[
                $(document).ready(function() {

                    $('.brand-logo').html($('img').attr('src', 'trex-logo.png'));

                });
            ]]>

		</scripts>



		<!-- custom styles -->
		<styles>
            <![CDATA[
            .brand-logo
            {
            		padding:0px !important;
           		    margin:0px !important;
            }
            .brand-logo img
            {
            		height: 100% !important;
    				width: 125% !important;
    				margin-left: 0px !important;
   					 margin-top: 0px !important;
    		}
            

                
            ]]>
		</styles>
	</configuration>
</extentreports>